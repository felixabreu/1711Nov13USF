package com.revature.hello;

public class Strings {
	public static void main(String[] args){
		String input="";
		System.out.println("Input a string. If done, just press enter.");
		input=System.in.toString();
	while (!input.equals("")){
		System.out.println("Input a string. If done, just press enter.");
		input=System.in.toString();
	}
	}
	
	
}
