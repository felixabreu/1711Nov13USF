package com.revature.hello;

public class AboutStrings {
	public static void main(String[] args) {

		String b = "Hello";
		String f = "World!";
		
		b = b.concat(" " + f);
		System.out.println(b);
		
		System.out.println(b.length());
	}
	
	public static String test(String x){
		
		return x.substring(2);
	}
}
